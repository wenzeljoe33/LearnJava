package day1.examples;

public class ExampleIf {

	public static void main(String[] args) {
		// If
		
		int x = 20;
		int y = 20; 
		
		if(x < y) {
			System.out.println("X is less than Y");
		}

		if(x == y) {
			System.out.println("X is equal to Y");
		}
	
		if(x > y) {
			System.out.println("X is greater than Y");
		}

}
